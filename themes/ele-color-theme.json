  "semanticHighlighting": true,

  "tokenColors": [
	  // javascript 
    {
      "scope": ["keyword.control.import.js","keyword.control.from.js"],
      "settings": {
        "foreground": "#e61515",
        "fontStyle": "bold"
      }
    },
	{
		"scope": ["keyword.operator.assignment.js","keyword.operator.relational.js","keyword.operator.comparison.js"],
		"settings": {
		  "foreground": "#ffffff",
		  "fontStyle": "italic"
		}
	  },
    {
      "scope": "variable.other.readwrite.alias.js",
      "settings": {
        "foreground": "#d8e615"
      }
    },
    {
      "scope": "storage.modifier.async.js",
      "settings": {
        "foreground": "#00ffc8"
      }
    },
    {
      "scope": "variable.parameter.js",
      "settings": {
        "foreground": "#ff8800",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.type.js",
      "settings": {
        "foreground": "#8d8d8d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type.function.js", "support.function.js","storage.type.function.arrow.js"],

      "settings": {
        "foreground": "#c8dd0f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.default.js",
      "settings": {
        "foreground": "#abac8f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.export.js",
      "settings": {
        "foreground": "#03a70b",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": "keyword.operator.new.js",
      "settings": {
        "foreground": "#00ff0d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string.quoted.double.js", "string.quoted.single.js", "string.template.js"],
      "settings": {
        "foreground": "#2ec5b1",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["punctuation.terminator.statement.js","punctuation.definition.string.template.begin.js","punctuation.definition.string.template.end.js"],
      "settings": {
        "foreground": "#ff00dd",
        "fontStyle": "bold"
      }
    },
	{
		"scope": ["punctuation.definition.template-expression.begin.js"],
		"settings": {
		  "foreground": "#f978fd",
		  "fontStyle": "bold"
		}
	  },
    {
      "scope": "entity.name.function.js",
      "settings": {
        "foreground": "#20b7f3",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "meta.function-call.js",
      "settings": {
        "foreground": "#e2e2e2"
      }
    },
    {
      "scope": ["variable.other.readwrite.js", "variable.other.object.js"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
	{
		"scope": ["variable.other.property.js"],
		"settings": {
		  "foreground": "#3bff86",
		  
		}
	  },
    {
      "scope": "punctuation.separator.key-value.js",
      "settings": {
        "foreground": "#ff8800",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.js",
      "settings": {
        "foreground": "#ffbb00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable.other.constant.js",
      "settings": {
        "foreground": "#2eb3a8",
        "fontStyle": "bold"
      }
    }
